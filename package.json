{
  "name": "lifeguard",
  "version": "0.3.1",
  "description": "lifeguard discord bot",
  "keywords": [
    "discord",
    "typescript",
    "lifeguard",
    "discord.js"
  ],
  "main": "./build/main.js",
  "scripts": {
    "build": "nest build",
    "compile": "swc src -d build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "prettier --check \"src/**/*.ts\"",
    "lint:fix": "prettier --write \"src/**/*.ts\"",
    "prebuild": "rimraf dist",
    "precompile": "pnpm run lint",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node --unhandled-rejections=strict --trace-warnings --trace-uncaught build/main.js"
  },
  "_moduleAliases": {
    "~": "build/"
  },
  "author": "Rauf",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ItsRauf/esmcord.git"
  },
  "types": "./build/mod.d.ts",
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@swc/cli": "^0.1.45",
    "@swc/core": "^1.2.59",
    "@types/node": "^14.17.1",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "dotenv": "^8.6.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.0",
    "typescript": "^4.3.2"
  },
  "engines": {
    "node": ">=12.2.0"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.18",
    "@nestjs/core": "^7.6.18",
    "@swc/helpers": "^0.2.12",
    "@types/module-alias": "^2.0.0",
    "@types/ws": "^7.4.4",
    "ansi-colors": "^4.1.1",
    "discord-api-types": "^0.12.1",
    "eris": "^0.15.1",
    "module-alias": "^2.2.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.1.0",
    "ts-node": "^9.1.1",
    "ws": "^7.4.6"
  }
}
