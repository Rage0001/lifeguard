{
  "name": "lifeguard",
  "version": "1.0.0",
  "description": "Administration assistant for your Discord server!",
  "main": "dist/index.js",
  "typings": "index.d.ts",
  "scripts": {
    "build": "tsc -p .",
    "build:watch": "tsc -w -p .",
    "start": "node procman.js",
    "start:dev": "nodemon --watch dist procman.js",
    "test": "tslint -c tslint.json -p tsconfig.json"
  },
  "engines": {
    "node": "10.x",
    "npm": "*"
  },
  "keywords": [
