    "bot",
    "lifeguard"
  ],
  "author": "Rauf",
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^5.7.1",
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "discord.js": "^11.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.7.5",
    "ms": "^2.1.2",
    "node-schedule": "^1.3.2"
  },
  "devDependencies": {
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.5.8",
    "@types/ms": "^0.7.30",
    "@types/node": "^12.6.2",
    "@types/node-schedule": "^1.2.4",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.3"
  }
